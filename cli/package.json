{
  "name": "@frojdagency/jewl-cli",
  "description": "CLI tool for Jewl. Import your components with ease",
  "version": "0.0.3",
  "author": "Mikael Engstr√∂m <mikael.engstrom@frojd.se>",
  "homepage": "https://github.com/Frojd/Frojd-Jewl/",
  "bugs": {
    "url": "https://github.com/Frojd/Frojd-Jewl/issues"
  },
  "licence": "MIT",
  "bin": {
    "jewl-cli": "bin/run"
  },
  "dependencies": {
    "@oclif/core": "^1.26.2",
    "@oclif/plugin-help": "^5.2.20",
    "classnames": "^2.5.1",
    "cli-ux": "^6.0.9",
    "fs-extra": "^10.1.0",
    "lmify": "^0.3.0",
    "nodegit": "^0.27.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^2.5.6",
    "@types/chai": "^4.3.11",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.82",
    "@types/nodegit": "^0.27.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chai": "^4.4.1",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-plugin-unicorn": "^40.1.0",
    "globby": "^11.1.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "oclif": "^2.7.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "jewl-cli",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "init": {
        "description": "Inititalize jewl"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Frojd/Frojd-Jewl.git"
  },
  "scripts": {
    "fixcode": "eslint . --ext .ts --config .eslintrc --fix",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "license": "MIT"
}
