{
  "name": "@frojdagency/jewl-cli",
  "description": "CLI tool for Jewl. Import your components with ease",
  "version": "0.0.3",
  "author": "Mikael Engstr√∂m <mikael.engstrom@frojd.se>",
  "homepage": "https://github.com/Frojd/Frojd-Jewl/",
  "bugs": {
    "url": "https://github.com/Frojd/Frojd-Jewl/issues"
  },
  "licence": "MIT",
  "bin": {
    "jewl-cli": "bin/run"
  },
  "dependencies": {
    "@oclif/core": "^3.19.5",
    "@oclif/plugin-help": "^6.0.14",
    "classnames": "^2.5.1",
    "fs-extra": "^11.2.0",
    "nodegit": "^0.28.0-alpha.24",
    "nypm": "^0.3.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@oclif/test": "^3.2.0",
    "@types/chai": "^4.3.11",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.20",
    "@types/nodegit": "^0.28.7",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "chai": "^5.1.0",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3.0.48",
    "eslint-plugin-unicorn": "^51.0.1",
    "globby": "^14.0.1",
    "mocha": "^10.3.0",
    "nyc": "^15.1.0",
    "oclif": "^4.4.17",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "jewl-cli",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "init": {
        "description": "Inititalize jewl"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Frojd/Frojd-Jewl.git"
  },
  "scripts": {
    "fixcode": "eslint . --ext .ts --config .eslintrc --fix",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "license": "MIT"
}
