{
  "name": "frojd-jewl",
  "version": "0.1.0",
  "description": "Storybook dev server for Jewl Component Library",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006 -s ./app/assets",
    "build-storybook": "build-storybook -s ./app/assets",
    "new": "node cli/component/cli.js new",
    "new:container": "node cli/component/cli.js new --container",
    "delete": "node cli/component/cli.js delete",
    "delete:container": "node cli/component/cli.js delete --container",
    "eslint": "eslint app",
    "eslint:fix": "eslint app --fix",
    "fixcode": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --write",
    "fixcode:test": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --list-different",
    "start": "node ./bin/dev-server",
    "svg": "svgo -f app/svg -q --config=app/svg/svgo.config.js",
    "test": "jest"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(test).js?(x)"
    ],
    "modulePaths": [
      "app"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.setup-test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:frojd/frojd-jewl.git"
  },
  "keywords": [
    "react",
    "scss",
    "frontend",
    "boilerplate",
    "storybook"
  ],
  "author": "FrÃ¶jd Interactive AB",
  "license": "MIT",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead",
    "ie>=11"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@storybook/addon-a11y": "^6.4.14",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.14",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.6",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.1",
    "node-sass": "^6.0.0",
    "prettier": "^2.3.0",
    "prop-types": "^15.7.2",
    "react-svg-loader": "^3.0.3",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "svgo": "^2.3.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "core-js": "^3.13.0",
    "i18next": "^20.3.0",
    "local-storage": "^2.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-i18next": "^11.9.0",
    "regenerator-runtime": "^0.13.7",
    "velocity-react": "^1.4.3"
  }
}
