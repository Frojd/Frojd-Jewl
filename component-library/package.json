{
  "name": "frojd-jewl",
  "version": "0.1.0",
  "description": "Storybook dev server for Jewl Component Library",
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://frojd.github.io/Frojd-Jewl",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "deploy": "npm run storybook:build -- --quiet && npm run deploy-storybook",
    "new": "node cli/component/cli.js new",
    "new:container": "node cli/component/cli.js new --container",
    "delete": "node cli/component/cli.js delete",
    "delete:container": "node cli/component/cli.js delete --container",
    "eslint": "eslint app",
    "eslint:fix": "eslint app --fix",
    "fixcode": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --write",
    "fixcode:test": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --list-different",
    "start": "node ./bin/dev-server",
    "svg": "svgo -f app/svg -q --config=app/svg/svgo.config.js",
    "jest": "jest",
    "test": "concurrently \"npm run jest -- --silent --noStackTrace\" \"npm run eslint -- --quiet\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:frojd/frojd-jewl.git"
  },
  "keywords": [
    "react",
    "scss",
    "frontend",
    "boilerplate",
    "storybook"
  ],
  "author": "FrÃ¶jd Interactive AB",
  "license": "MIT",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead",
    "ie>=11"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.5.10",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.2",
    "concurrently": "^7.0.0",
    "css-loader": "^5.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.6.4",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jest-fetch-mock": "^3.0.3",
    "node-sass": "^6.0.0",
    "prettier": "^2.3.0",
    "prop-types": "^15.7.2",
    "react-svg-loader": "^3.0.3",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "svgo": "^2.3.0"
  },
  "dependencies": {
    "@whitespace/storybook-addon-html": "^5.0.0",
    "classnames": "^2.3.1",
    "core-js": "^3.13.0",
    "i18next": "^20.3.0",
    "local-storage": "^2.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-i18next": "^11.9.0",
    "regenerator-runtime": "^0.13.7",
    "velocity-react": "^1.4.3"
  }
}
