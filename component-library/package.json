{
  "name": "frojd-jewl",
  "version": "0.1.0",
  "description": "Storybook dev server for Jewl Component Library",
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://frojd.github.io/Frojd-Jewl",
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "deploy-storybook": "gh-pages -d storybook-static",
    "deploy": "npm run storybook:build -- --quiet && npm run deploy-storybook",
    "new": "node cli/component/cli.js new",
    "new:container": "node cli/component/cli.js new --container",
    "delete": "node cli/component/cli.js delete",
    "delete:container": "node cli/component/cli.js delete --container",
    "eslint": "eslint app",
    "eslint:fix": "eslint app --fix",
    "fixcode": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --write",
    "fixcode:test": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --list-different",
    "start": "node ./bin/dev-server",
    "svg": "svgo -f app/inline-svg -q --config=app/inline-svg/svgo.config.js",
    "jest": "jest",
    "test": "concurrently \"npm run jest -- --silent --noStackTrace\" \"npm run eslint -- --quiet\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:frojd/frojd-jewl.git"
  },
  "keywords": [
    "react",
    "scss",
    "frontend",
    "boilerplate",
    "storybook"
  ],
  "author": "FrÃ¶jd Interactive AB",
  "license": "MIT",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead",
    "ie>=11"
  ],
  "devDependencies": {
    "@storybook/addon-a11y": "^7.6.17",
    "@storybook/addon-actions": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/nextjs": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@swc/core": "^1.4.2",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@whitespace/storybook-addon-html": "^5.1.6",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "gh-pages": "^3.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^16.0.0",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "storybook": "^7.6.17",
    "style-loader": "^3.3.4",
    "svgo": "^3.2.0"
  },
  "dependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^18.2.57",
    "classnames": "^2.5.1",
    "framer-motion": "^11.0.5",
    "i18next": "^23.9.0",
    "local-storage": "^2.0.0",
    "next": "^14.1.0",
    "next-i18next": "^15.2.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "sass": "^1.71.0",
    "sharp": "^0.33.2"
  }
}
