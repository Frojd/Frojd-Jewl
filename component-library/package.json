{
    "name": "frojd-jewl",
    "version": "0.1.0",
    "description": "An opinionated react starterkit",
    "main": "app/index.js",
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "build": "webpack --mode=development",
        "build:prod": "cross-env NODE_ENV=production webpack --mode=production -p",
        "eslint": "eslint app",
        "eslint:fix": "eslint app --fix",
        "fixcode": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --write",
        "fixcode:test": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --list-different",
        "multiwatch": "concurrently \"npm run watch\" \"npm run start\"",
        "new": "node cli/component/cli.js new",
        "new:container": "node cli/component/cli.js new --container",
        "delete": "node cli/component/cli.js delete",
        "delete:container": "node cli/component/cli.js delete --container",
        "ssr": "cross-env webpack --mode=development --config ./webpack.ssr.config.js",
        "ssr:prod": "cross-env NODE_ENV=production webpack --mode=production --config ./webpack.ssr.config.js -p",
        "ssr:watch": "cross-env NODE_ENV=production webpack --mode=development --config ./webpack.ssr.config.js --watch",
        "start": "node ./bin/dev-server",
        "storybook": "start-storybook -p 9001 -s ./app/assets",
        "storybook:out": "build-storybook -o dist/storybook",
        "svg": "svgo -f app/svg -q --config=app/svg/.svgo.yml",
        "test": "jest",
        "watch": "webpack --mode=development --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:frojd/frojd-jewl.git"
    },
    "keywords": [
        "webpack",
        "sass",
        "scss",
        "hmr",
        "react",
        "frontend",
        "boilerplate",
        "starterkit",
        "cli",
        "scaffold",
        "storybook"
    ],
    "author": "FrÃ¶jd",
    "license": "MIT",
    "browserslist": [
        "last 1 version",
        "> 1%",
        "not dead",
        "ie>=11"
    ],
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less|scss)$": "identity-obj-proxy"
        },
        "setupFiles": [
            "./.setup-test.js"
        ],
        "testMatch": [
            "**/?(*.)(test).js?(x)"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/dist/",
            "/internals/"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "testURL": "http://localhost"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.8",
        "@babel/core": "^7.12.9",
        "@storybook/addon-a11y": "^6.2.9",
        "@storybook/addon-actions": "^6.2.9",
        "@storybook/addon-essentials": "^6.2.9",
        "@storybook/addon-knobs": "^6.2.9",
        "@storybook/addon-links": "^6.2.9",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/addon-storysource": "^6.2.9",
        "@storybook/react": "^6.2.9",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.1",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.2",
        "css-loader": "^5.0.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "eslint": "^7.14.0",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "expose-loader": "^1.0.2",
        "file-loader": "^6.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
        "markdown-loader": "^6.0.0",
        "node-sass": "^5.0.0",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-nested": "^5.0.1",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^2.2.0",
        "prop-types": "^15.7.2",
        "react-svg-loader": "^3.0.3",
        "sass-loader": "^10.1.0",
        "style-loader": "^2.0.0",
        "svgo": "^1.3.2"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "core-js": "^3.8.0",
        "i18next": "^19.8.4",
        "local-storage": "^2.0.0",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-i18next": "^11.7.3",
        "regenerator-runtime": "^0.13.7",
        "velocity-react": "^1.4.3"
    }
}
