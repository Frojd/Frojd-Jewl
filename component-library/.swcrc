{
    "jsc": {
        "target": "es5",
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": true
        },
        "keepClassNames": true,
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        }
    },
    "module": {
        "type": "commonjs",
        "noInterop": true
    }
}
